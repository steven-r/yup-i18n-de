{
  "name": "@stephen-r/yup-i18n-de",
  "version": "1.0.1",
  "main": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && typedoc",
    "test": "jest && codecov",
    "semantic-release": "semantic-release",
    "clean": "shx rm -rf node_modules/ dist/ docs/",
    "typedoc": "typedoc --options typedoc.json --theme node_modules/typedoc-neo-theme/bin/default && touch docs/.nojekyll"
  },
  "repository": "steven-r/yup-i18n-de",
  "bugs": {
    "url": "https://github.com/steven-r/yup-i18-de/issues"
  },
  "keywords": [
    "yup",
    "i18n"
  ],
  "author": "Stephen Reindl",
  "license": "MIT",
  "description": "A small libary that sets the locale for yup to German",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/yup": "^0.26.32",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.4",
    "shx": "^0.3.2",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.16.11",
    "typedoc-neo-theme": "^1.0.6",
    "typescript": "^3.8.3",
    "yup": "^0.28.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
